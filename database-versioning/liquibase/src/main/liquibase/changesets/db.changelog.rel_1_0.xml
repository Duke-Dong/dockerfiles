<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

    <changeSet id="1" author="Rango">
        <createTable tableName="Author2">
            <column name="author_id" type="int"></column>
            <column name="first_name" type="varchar(50)"></column>
            <column name="last_name" type="varchar(50)"></column>
            <column name="email" type="varchar(50)"></column>
        </createTable>
    </changeSet>
    <changeSet id="2" author="Rango">
        <createTable tableName="Genre2">
            <column name="genre_id" type="int"></column>
            <column name="genre" type="varchar(50)"></column>
        </createTable>
    </changeSet>
</databaseChangeLog>

